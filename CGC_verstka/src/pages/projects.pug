extends common/layout.pug



include common/mixins.pug


block title 
	| projects CGC Carat Global Construction



block content
	+header('header--light')
	section.projects
		.container
			.projects__title
				p
					| Наши 
					span.text-accent проекты
				a.btn-primary.btn-primary--outline.btn-primary--icon.icon-arrow-right(href='#')  Все проекты
			.projects__filter
				.projects__filter-top
					.projects__filter-switcher
						.can-toggle.demo-rebrand-1
							input#d(type='checkbox')
							label(for='d')
								.can-toggle__switch(data-checked='Текущие' data-unchecked='Реализованные')
					.projects__filter-buttons
						.projects__filter-button Все
						.projects__filter-button Проектирование
						.projects__filter-button Сбор исходно-разрешительной документации
					.projects__filter-next
						span Еще
						i.icon-arrow-right 
				.projects__filter-bottom
					.projects__filter-buttons.hidden-js
						.projects__filter-button Архитектурное проектирование
						.projects__filter-button Проектирование зданий
						.projects__filter-button Проектирование инженерных коммуникаций
						.projects__filter-button Дизайн проект
						.projects__filter-button Проектирование систем безопасности и связи
						.projects__filter-button Ландшафтное проектирование
						.projects__filter-button Строительство
						.projects__filter-button Сопровождение
